                        <a class="dropdown-item" href="#">Jouets</a>
                         
                       
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href="#">Artisans</a>
                    </li>

                     <li class="nav-item">
                        <a class="nav-link" href="#">L'équipe</a>
                    </li>

                    <li class="nav-item">
                        <a class="nav-link" href="#">Nous contacter</a>
                    </li>


                </ul>
                <form class="form-inline my-2 my-lg-0">
                    <input class="form-control mr-sm-2" type="search" placeholder="" aria-label="Search">
                    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Trouver</button>
                </form>
                <span class="log"><a href=""><img src="{{asset('img/log.png')}}" width="50" alt=""></a></span>
                <span class="cart"><img src="{{asset('img/cart.png')}}" width="50" alt=""></span>
            </div>
        </nav>

        <div class="container-fluid">
        {% block body %}{% endblock %}
        </div>

        {% block javascripts %}{% endblock %}
        <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
